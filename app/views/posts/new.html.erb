<%= form_with model: @post do |f| %>
  <%= f.label :art_name %>
  <%= f.text_field :art_name %>
  <%= f.fields_for :arts do |i| %>
    <%= i.file_field :image %>
  <% end %>
  <%# category %>
  <%= f.select :category_id, Category.all.map{|o| [o.name, o.id]} %>
  <%= f.submit "投稿", class: "btn btn-primary" %>
<% end %>
